{
  "expo": {
    "name": "Alarmist",
    "slug": "Alarmist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "bundleIdentifier": "com.twotwofive.alarmist",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "splash": {
        "image": "./assets/splash-screens/ios/splash.png",
        "resizeMode": "cover"
      }
    },
    "android": {
      "package": "com.twotwofive.alarmist",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#171717"
      },
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "READ_CALENDAR",
        "WRITE_CALENDAR"
      ]
    }
  }
}